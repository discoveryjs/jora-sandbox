{
    "name": "jora-sandbox",
    "version": "1.1.0",
    "description": "Sandbox for Jora (a JSON query language)",
    "author": "Roman Dvornov <rdvornov@gmail.com>",
    "license": "MIT",
    "repository": "discoveryjs/jora-sandbox",
    "keywords": [
        "jora",
        "sandbox"
    ],
    "main": "./index.js",
    "scripts": {
        "start": "discovery",
        "lint": "eslint *.js src",
